#include <limits.h>
#include <stdbool.h>

int myAtoi(char *s) {
    long result = 0;
    int sign = 1;
    int i = 0;
    while (s[i] == ' ') {
        i++;
    }
    if (s[i] == '-') {
        sign = -1;
        i++;
    } else if (s[i] == '+') {
        i++;
    }
    while (s[i] >= '0' && s[i] <= '9') {
        int digit = s[i] - '0';
        if (sign == 1) {
            if (result > INT_MAX / 10) {
                return INT_MAX;
            }
            if (result == INT_MAX / 10 && digit > 7) {
                return INT_MAX;
            }
        }
        if (sign == -1) {
            if (result > INT_MAX / 10) { 
                return INT_MIN;
            }
            if (result == INT_MAX / 10 && digit > 8) {
                return INT_MIN;
            }
        }
        
        result = result * 10 + digit;
        i++;
    }
    return (int)(sign * result);
}
